{
    "name": "diablomedia/browser-age",
    "type": "library",
    "description": "Show release date of user's browser based on parsed properties from useragent",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Klehr",
            "email": "jay@diablomedia.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "localheinz/composer-normalize": "^1.0",
        "phpro/grumphp": "^0.14.3",
        "phpunit/phpunit": "^6.5 || ^7.3"
    },
    "autoload": {
        "psr-4": {
            "BrowserAge\\": "src/BrowserAge/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrowserAgeTest\\": "src/BrowserAgeTest/"
        }
    },
    "scripts": {
        "normalize": "composer normalize --dry-run",
        "style-check": "php-cs-fixer fix --dry-run -vv",
        "test": "phpunit",
        "test-with-coverage": "phpunit --coverage-clover=clover.xml"
    }
}
