{
    "name": "diablomedia/browser-age",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Klehr",
            "email": "jay@diablomedia.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "BrowserAge\\": "src/BrowserAge/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrowserAgeTest\\": "src/BrowserAgeTest/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.3"
    },
    "scripts": {
        "test": "phpunit",
        "test-with-coverage": "phpunit --coverage-clover=clover.xml",
        "style-check": "php-cs-fixer fix --dry-run -vv"
    }
}
